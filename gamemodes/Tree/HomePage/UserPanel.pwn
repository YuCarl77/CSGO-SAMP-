N::PlayerLevelUpdate(playerid,amount)
{
    Account[playerid][_Exp]+=amount;
    new index=0;
    while(Account[playerid][_Exp]>Level[index][_Exp])
    {
        index++;
    }
    if(Account[playerid][_Level]!=index-1)
    {
        Account[playerid][_Level]=index-1;
        SetPlayerScore(playerid,Account[playerid][_Level]);
    }
	formatex128("UPDATE `"MYSQL_DB_USER"` SET  `等级` =  '%i',`经验` =  '%i' WHERE  `"MYSQL_DB_USER"`.`密匙` ='%s'",Account[playerid][_Level],Account[playerid][_Exp],Account[playerid][_Key]);
	mysql_query(User@Handle,string128,false);
    UpdatePlayerLevelDraws(playerid);
	return 1;
}
N::ShowPlayerLevelDraws(playerid)
{
	HidePlayerLevelDraws(playerid);
    TextDrawShowForPlayer(playerid,AccountLevelTextDraw);
	PlayerLevelAvatar[playerid]=CreatePlayerLevelAvatar(playerid,Level[Account[playerid][_Level]][_Mod@Avatar]);
	PlayerLevelBar[playerid]=CreatePlayerLevelBar(playerid);
	
    PlayerRightRotArrow[playerid]=CreatePlayerRightRotArrow(playerid);
    PlayerLeftRotArrow[playerid]=CreatePlayerLeftRotArrow(playerid);
	PlayerLevelTextDrawShow[playerid]=true;
	return 1;
}
N::HidePlayerLevelDraws(playerid)
{
	RestPlayerLevelDraws(playerid,true);
 	TextDrawHideForPlayer(playerid,AccountLevelTextDraw);
	return 1;
}
N::UpdatePlayerLevelDraws(playerid)
{
	if(PlayerLevelTextDrawShow[playerid]==true)
	{
	    //printf("%i,%i",Account[playerid][_Exp],Level[Account[playerid][_Level]+1][_Exp]);
		new Float:size=floatmul(floatdiv(Account[playerid][_Exp],Level[Account[playerid][_Level]+1][_Exp]),230.0);
	    PlayerTextDrawTextSize(playerid,PlayerLevelBar[playerid], 406.500000+size , 308.500000);
	    PlayerTextDrawShow(playerid,PlayerLevelBar[playerid]);
		PlayerTextDrawSetPreviewModel(playerid,PlayerLevelAvatar[playerid],Level[Account[playerid][_Level]][_Mod@Avatar]);
		PlayerTextDrawShow(playerid,PlayerLevelAvatar[playerid]);
	}
	return 1;
}
N::RestPlayerLevelDraws(playerid,bool:destroy)
{
    PlayerLevelTextDrawShow[playerid]=false;
    if(destroy==true)
    {
        SetSVarString("PTDD","PlayerLevelAvatar");
		if(PlayerLevelAvatar[playerid]!=PlayerText:INVALID_TEXT_DRAW)PlayerTextDrawDestroy(playerid,PlayerLevelAvatar[playerid]);
        SetSVarString("PTDD","PlayerLevelBar");
		if(PlayerLevelBar[playerid]!=PlayerText:INVALID_TEXT_DRAW)PlayerTextDrawDestroy(playerid,PlayerLevelBar[playerid]);
		SetSVarString("PTDD","PlayerRightRotArrow");
		if(PlayerRightRotArrow[playerid]!=PlayerText:INVALID_TEXT_DRAW)PlayerTextDrawDestroy(playerid,PlayerRightRotArrow[playerid]);
        SetSVarString("PTDD","PlayerLeftRotArrow");
		if(PlayerLeftRotArrow[playerid]!=PlayerText:INVALID_TEXT_DRAW)PlayerTextDrawDestroy(playerid,PlayerLeftRotArrow[playerid]);
	}
    PlayerLevelAvatar[playerid]=PlayerText:INVALID_TEXT_DRAW;
    PlayerLevelBar[playerid]=PlayerText:INVALID_TEXT_DRAW;
    
    PlayerRightRotArrow[playerid]=PlayerText:INVALID_TEXT_DRAW;
    PlayerLeftRotArrow[playerid]=PlayerText:INVALID_TEXT_DRAW;
    PlayerClickRotMode[playerid]=0;
	return 1;
}
