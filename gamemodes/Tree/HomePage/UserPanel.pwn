N::ShowPlayerAccountPanelRegister(playerid)//注册前面板显示
{
	TextDrawShowForPlayer(playerid,AccountPanelTextDraw[0]);
	TextDrawShowForPlayer(playerid,AccountPanelTextDraw[1]);
 	PlayerPanelAvatar[playerid]=CreatePlayerPanelAvatar(playerid,Skins[Account[playerid][_Skin@Avatar]][_Mod@Avatar]);
	PlayerPanelTextDrawShow[playerid]=true;
	return 1;
}
N::ShowPlayerAccountPanelLogin(playerid)//登陆前面板显示
{
	TextDrawShowForPlayer(playerid,AccountPanelTextDraw[0]);
	TextDrawShowForPlayer(playerid,AccountPanelTextDraw[2]);
	PlayerPanelAvatar[playerid]=CreatePlayerPanelAvatar(playerid,Skins[Account[playerid][_Skin@Avatar]][_Mod@Avatar]);
	PlayerPanelTextDrawShow[playerid]=true;
	return 1;
}
N::ShowPlayerAccountPanel(playerid,bool:ShowAvatar)//登陆注册后面板显示
{
    HidePlayerAccountPanel(playerid);

    for(new i=0;i<MAX_ACCOUNTPTD;i++)if(i!=1&&i!=2)TextDrawShowForPlayer(playerid,AccountPanelTextDraw[i]);
    if(ShowAvatar==true)
    {
		SetSVarString("PTDD","PlayerPanelAvatar");
    	if(PlayerPanelAvatar[playerid]!=PlayerText:INVALID_TEXT_DRAW)PlayerTextDrawDestroy(playerid,PlayerPanelAvatar[playerid]);
    	PlayerPanelAvatar[playerid]=PlayerText:INVALID_TEXT_DRAW;
    	PlayerPanelAvatar[playerid]=CreatePlayerPanelAvatar(playerid,Skins[Account[playerid][_Skin@Avatar]][_Mod@Avatar]);
    }
    PlayerPanelTextDrawShow[playerid]=true;
	return 1;
}
N::HidePlayerAccountPanel(playerid)
{
	forex(i,MAX_ACCOUNTPTD)TextDrawHideForPlayer(playerid,AccountPanelTextDraw[i]);
	return 1;
}
N::RestPlayerAccountPanelDraws(playerid,bool:destroy)//初始化玩家面板DRAWS
{
    PlayerPanelTextDrawShow[playerid]=false;
	if(destroy==true)
	{
	    SetSVarString("PTDD","PlayerPanelAvatar");
		if(PlayerPanelAvatar[playerid]!=PlayerText:INVALID_TEXT_DRAW)PlayerTextDrawDestroy(playerid,PlayerPanelAvatar[playerid]);
	}
	PlayerPanelAvatar[playerid]=PlayerText:INVALID_TEXT_DRAW;
	return 1;
}
